{
  "name": "pr-of-it/t4",
  "description": "T4 framework",
  "keywords": [
    "php",
    "framework",
    "t4"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.0",
    "twig/twig": "1.*",
    "phpmailer/phpmailer": "5.*",
    "psr/log": "1.*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phing/phing": "2.*",
    "phpunit/phpunit": "7.*",
    "phpunit/dbunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "T4\\": "framework/"
    },
    "files": [
      "framework/boot.php",
      "framework/app.php"
    ]
  }
}
